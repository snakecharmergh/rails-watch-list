<div class="container">
  <h1>My Lists:</h1>
  <ul style="list-style-type: none">
    <% @lists.each do |list| %>
      <li><%= list.id %>
      <%= link_to list.name, list_path(list), class: "text-decoration-none text-black" %>
    <% end %>
  </ul>
  <p><%= link_to "Add a new list", new_list_path, class: "text-decoration-none" %></p>




</div>
